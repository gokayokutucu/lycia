{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ApplicationId": "SampleOrderConsumer",
  "Lycia": {
    "Outbox": {
      "TableSuffix": "OutboxMessages"
    },
    "EventBus": {
      "Provider": "RabbitMQ",
      "ConnectionString": "amqp://guest:guest@127.0.0.1:5672/"
    },
    "EventStore": {
      "Provider": "Redis",
      "ConnectionString": "127.0.0.1:6379",
      "LogMaxRetryCount" : 5
    }
  }
}
