{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ApplicationId": "SampleOrderConsumer",
  "Lycia": {
    "Outbox": {
      "TableSuffix": "OutboxMessages"
    },
    "EventBus": {
      "Provider": "RabbitMQ",
      "ConnectionString": "amqp://guest:guest@127.0.0.1:5672/"
    },
    "EventStore": {
      "Provider": "Redis",
      "ConnectionString": "127.0.0.1:6379",
      "LogMaxRetryCount" : 5
    },
    "Retry": {
      "MaxRetryAttempts": 5,
      "BackoffType": "Exponential",
      "Delay": "00:00:01",
      "MaxDelay": "00:00:10",
      "UseJitter": true
    },
    "Logging": {
      "MinimumLevel": "Information",
      "IncludeMessageHeaders": true,
      "IncludeMessagePayload": false,
      "PayloadMaxLength": 2048,
      "RedactedHeaderKeys": [ "Authorization", "X-Api-Key" ],
      "StartTemplate": null,
      "SuccessTemplate": null,
      "ErrorTemplate": null
    }
  }
}
